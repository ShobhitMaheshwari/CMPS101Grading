Check if graph is constructed and destructed properly
gcc -std=c99 -Wall -c Graph_addEdge4.c > w 2>&1
gcc -std=c99 -Wall -c main.c > w  2>&1
gcc -std=c99 -Wall -o Graph_addEdge4 main.o Graph_addEdge4.o ../List.o ../Graph.o -pthread -lrt &> r
./Graph_addEdge4 >  Graph_addEdge4.out
valgrind --leak-check=full --log-file="memoryLeak" ./Graph_addEdge4
