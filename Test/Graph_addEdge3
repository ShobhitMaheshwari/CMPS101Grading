Check if graph is constructed and destructed properly
gcc -std=c99 -Wall -c Graph_addEdge3.c > w 2>&1
gcc -std=c99 -Wall -c main.c > w  2>&1
gcc -std=c99 -Wall -o Graph_addEdge3 main.o Graph_addEdge3.o ../List.o ../Graph.o -pthread -lrt &> r
./Graph_addEdge3 >  Graph_addEdge3.out
valgrind --leak-check=full --log-file="memoryLeak" ./Graph_addEdge3
